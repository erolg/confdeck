<h1>Listing rooms for conference <%= @conference.name %></h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.name %></td>
    <td><%= link_to 'Show', ["admin", room] %></td>
    <td><%= link_to 'Edit', editadmin_room_path(room) %></td>
    <td><%= link_to 'Destroy', ["admin", room], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Room', new_admin_room_path %>
